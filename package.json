{
  "scripts": {
    "start": "parcel index.pug",
    "build": "parcel build index.pug --public-url .",
    "predeploy": "npm run build && dest=$(mktemp -d) && cp -r dist/* $dest && git checkout master && git clean -fd && cp -r $dest/* .",
    "deploy": "git add -A && git commit -m \"Changes deploy $(git rev-parse dev)\" && git push"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "less": "^3.0.0",
    "parcel-bundler": "^1.1.0",
    "parcel-plugin-pug": "^0.2.7",
    "parcel-plugin-vue": "^1.5.0",
    "pug": "^2.0.0-rc.4",
    "vue": "^2.5.13",
    "vue-template-compiler": "^2.5.13"
  },
  "dependencies": {
    "babel-preset-stage-0": "^6.24.1",
    "bootstrap-vue": "^2.0.0-rc.1",
    "vue-progress-path": "^0.0.2"
  }
}
